{
    "name": "Hacktoberfest Extension",
    "version": "1.0",
    "description": "Adds a hacktoberfest button to easily show hacktoberfest issues!",
    "content_scripts": [
        {
            "matches": [
                "https://*.github.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "manifest_version": 2
}